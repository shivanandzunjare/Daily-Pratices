package pkg3;

public class MethodOverriding {
	
	public class Animal{
		public void sound() {
			System.out.println("general sound");
		}
		
	}
	class Dog extends Animal{
		@Override
		public void sound() {
			System.out.println("general sound");
		}
		
		
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Animal a = new Dog();

	}

}
