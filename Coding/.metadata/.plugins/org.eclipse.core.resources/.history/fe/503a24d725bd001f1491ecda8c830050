package pkg3;

public class Dog extends Animal{
	


		@Override
		public void sound() {
			System.out.println("dog barking");
		}
		
		
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Animal a =  new Dog();
//		Dog.sound();
//		a.sound();
		
		if(a instanceof Dog) {
			Dog d = (Dog )a;
			d.sound();
		}

	}

}
